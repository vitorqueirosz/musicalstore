{
  "name": "musicalstore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.0",
    "next": "11.1.2",
    "polished": "^4.1.3",
    "querystring": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@svgr/cli": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.27",
    "@types/react-slick": "^0.23.6",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "apollo": "^2.33.9",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "4.4.3"
  }
}
